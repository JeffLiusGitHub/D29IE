
@{
    ViewBag.Title = "Crash Analysis";
    ViewBag.BodyStyle = "body-no-banner";
}

<div id="analysis" class="section lb">
    <div class="container">
        <div class="section-title text-center">
            <h3>Crash Analysis</h3>
            <p>The Crash Analysis presents the most risky time intervals of a day across the top 10 worst affected cities in Victoria. It also provides the road geometory for crash locations as well as the safest and the most usafe days of the week to ride a bicycle.</p>
        </div><!-- end title -->

        <div class="row">
            <div class="col-md-12">

                <iframe src=" https://public.tableau.com/views/Dashboard_wide_view/Dashboard2?:showVizHome=no&:embed=true &:toolbar=no"
                        width="1025" height="830">
                </iframe>
            </div>

        </div>

        <div class="row mt-5 pt-5">
            <div class="col-md-12">
                <div class="text-center">
                    <a class="sim-btn hvr-bounce-to-top" href="@Url.Action("Index", "Home")">
                        <i class="fa fa-long-arrow-left pr-2"></i>
                        <span>Back To Home</span>
                    </a>
                </div>
            </div>
        </div>

    </div>
</div>